{
	"name": "worldcountries",
	"version": "0.1.0",
	"private": true,
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"vue"
		],
		"transform": {
			".*\\.(vue)$": "vue-jest",
			"^.+\\.js$": "babel-jest"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		],
		"watchPathIgnorePatterns": [
			"/node_modules/"
		],
		"collectCoverage": true,
		"coverageDirectory": "./coverage/",
		"collectCoverageFrom": [
			"**/*.{js,vue}",
			"!**/node_modules/**",
			"!**/coverage/**",
			"!**/babel.config.js"
		]
	},
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"test": "jest --watch",
		"coverage": "jest --coverage",
		"deploy": "sh deploy.sh"
	},
	"dependencies": {
		"axios": "^1.12.2",
		"bulma": "^0.9.4",
		"core-js": "^3.46.0",
		"vue": "^2.7.16",
		"vuex": "^3.6.2"
	},
	"devDependencies": {
		"@babel/core": "^7.12.10",
		"@babel/preset-env": "^7.12.11",
		"@vue/cli-plugin-babel": "^3.12.1",
		"@vue/cli-plugin-eslint": "^3.12.1",
		"@vue/cli-service": "~4.5.0",
		"@vue/test-utils": "^2.4.6",
		"babel-core": "^7.0.0-bridge.0",
		"babel-eslint": "^10.1.0",
		"babel-jest": "^30.2.0",
		"eslint": "^7.32.0",
		"eslint-plugin-jest": "^24.1.3",
		"eslint-plugin-vue": "^7.20.0",
		"jest": "^30.2.0",
		"jest-serializer-vue": "^2.0.2",
		"regenerator-runtime": "^0.13.7",
		"vue-template-compiler": "^2.7.16"
	},
	"eslintConfig": {
		"root": true,
		"plugins": [
			"jest"
		],
		"env": {
			"node": true,
			"jest": true,
			"browser": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
